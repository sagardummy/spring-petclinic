pipeline {
    agent any {
    stages {
        stage ('source'){
            steps{
                git 'https://github.com/sagardummy/spring-petclinic.git'
            }
        }
        stage ('package'){
            steps{
                sh 'mvn package'
            }
        }
    } 
        
    }
}






node {
    stage ('scm'){
        git 'https://github.com/sagardummy/spring-petclinic.git'
    }
    stage ('build'){
        sh 'mvn package'
    }
}